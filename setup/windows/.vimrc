set sm
syntax on
set history=1000
set iminsert=0
set imsearch=0
set noea
set title
set autowrite
set showcmd
set nowrap
set nobackup
set esckeys
set ru
set nu
syntax enable
set mps+=<:>
set scs
set ic
set cindent

map <F3> :w<CR>
set nocompatible
filetype off

set tabstop=4
